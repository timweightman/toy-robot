{
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error", {
        "singleQuote": true,
        "printWidth": 100,
        "semi": true
      }
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}